CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE ALUNOS(CODIGO INTEGER NOT NULL PRIMARY KEY,NAME VARCHAR(50),SENHA VARCHAR(15),SEXO VARCHAR(15),DATA DATE,RESPONSAVEL VARCHAR(50),EMAIL VARCHAR(50),TELEFONE VARCHAR(15),ENDERECO VARCHAR(80))
CREATE MEMORY TABLE PROFESSOR(CODIGO INTEGER NOT NULL PRIMARY KEY,NAME VARCHAR(50),SENHA VARCHAR(15),SEXO VARCHAR(15),DATA DATE,ESPECIALIZACAO VARCHAR(50),EMAIL VARCHAR(50),TELEFONE VARCHAR(15),ENDERECO VARCHAR(80))
CREATE MEMORY TABLE QUESTION(CODIGO INTEGER NOT NULL PRIMARY KEY,ENUNCIADO VARCHAR(200),A VARCHAR(100),B VARCHAR(100),C VARCHAR(100),D VARCHAR(100),RESP VARCHAR(2))
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 10
