CREATE MEMORY TABLE ALUNOS(CODIGO INTEGER NOT NULL PRIMARY KEY,NOME VARCHAR(50),SENHA VARCHAR(50),SEXO VARCHAR(50),DATA VARCHAR(50),EMAIL VARCHAR(50),TELEFONE VARCHAR(50),ENDERECO VARCHAR(50),RESPONSAVEL VARCHAR(50))
CREATE MEMORY TABLE PROFESSORES(CODIGO INTEGER NOT NULL PRIMARY KEY,NOME VARCHAR(50),SENHA VARCHAR(50),SEXO VARCHAR(50),DATA VARCHAR(50),EMAIL VARCHAR(50),TELEFONE VARCHAR(50),ENDERECO VARCHAR(50),ESPECIALIZACAO VARCHAR(50))
CREATE MEMORY TABLE QUESTIONS(CODIGO INTEGER NOT NULL PRIMARY KEY,ENUNCIADO VARCHAR(200),A VARCHAR(20),B VARCHAR(20),C VARCHAR(20),D VARCHAR(20),RESP CHAR(1))
